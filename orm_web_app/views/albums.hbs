<div class="container">
  <div class="row">
    <div class="col">
      <table class="table">
        <thead class="thead-dark">
          <tr>
            <th scope="col">Album</th>
            <th scope="col">Artist</th>
          </tr>
        </thead>
        <tbody>
          {{#each albums}}
          <tr>
            <td>{{Title}}</td>
            <td>{{Name}}</td>
          </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
</div>